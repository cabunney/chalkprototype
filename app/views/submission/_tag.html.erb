<%= stylesheet_link_tag 'tag', :media => 'screen' %>
<td><div id = "tag_editor">
	<span id = 'tag_space'>
	<% @tags.each do |t| %>
		<span class = "tag" id = <%= "'tag_#{t.id}'".html_safe%> ><span class = "tag_name"><%= t.name %></span> 
		<a href="#" onclick="$(this).parent().remove()"> x</a>
		</span>
	<% end %>
	</span>
	<input type="text" id = "tag_editor_input"/>
</div></t>

